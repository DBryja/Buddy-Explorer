<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Document</title>
    <!-- function to find form post errors  -->
    <% function getError(prop){ try{ const searchIndex = errors.findIndex((item) => item.param == prop);
    return(errors[searchIndex].msg); } catch(err){ return ""; } } %>
  </head>
  <body>
    <%- include("../components/header"); %><!-- -->
    <div class="measure-wd"></div>
    <div class="wrapper--signup signup">
      <form id="signup" class="signup--form" method="POST" enctype="multipart/form-data">
        <div class="form--step form--step--1">
          <h1 class="margin">Dane logowania</h1>
          <section>
            <div class="title-input">
              <span class="title-input--title error"><%= getError("email")%></span>
              <input
                type="text"
                placeholder="adres e-mail"
                class="email input input--lb-title"
                name="email"
                data-number="0"
                required
              />
            </div>
            <div class="title-input">
              <span class="title-input--title error"><%= getError("nickname")%></span>
              <input
                type="text"
                placeholder="nazwa użytkownika"
                class="nickname input input--lb-title"
                name="nickname"
                data-number="0"
                required
              />
            </div>
            <div class="title-input">
              <span class="title-input--title error"><%= getError("password")%></span>
              <div class="password_container">
                <input
                  type="password"
                  placeholder="hasło"
                  class="password input input--lb-title"
                  name="password"
                  data-number="0"
                  blockid="1"
                  required
                />
                <div class="passwordVis" blockid="1"></div>
              </div>
            </div>
            <div class="title-input">
              <span class="error title-input--title"><%= getError("passwordConfirmation")%></span>
              <div class="password_container">
                <input
                  type="password"
                  placeholder="powtórz hasło"
                  class="password input input--lb-title"
                  name="passwordConfirmation"
                  data-number="0"
                  blockid="2"
                  required
                />
                <div class="passwordVis" blockid="2"></div>
              </div>
            </div>
          </section>
          <span></span>
        </div>

        <div class="form--step form--step--2">
