    <!-- SCRIPTS -->
    <% if (typeof libs !== 'undefined') { %> <% for (let lib of libs) { %>
    <script src="/libs/<%= lib %>.js" type="text/javascript"></script>
    <% } %> <% } %>
    <script>
      document.addEventListener("DOMContentLoaded", () => {
        const inputs = [
          document.querySelectorAll(".form--step--1 input"),
          document.querySelectorAll(".form--step--2 input"),
          document.querySelectorAll(".form--step--3 input"),
        ];

        //county Searchbars service
        removeItemListener();
        document.querySelectorAll(".prevent").forEach((item) => {
          item.addEventListener("click", (e) => e.preventDefault());
        });
        addCountyListener(inputs[1], ".form--step--2 input");
        addCityListener(inputs[2], ".form--step--3 input");

        let countiesSearchbars = document.querySelectorAll(".county_autocomplete");
        let citiesSearchbars = document.querySelectorAll(".city_autocomplete");

        SearchbarsForeach(countiesSearchbars, "counties");
        SearchbarsForeach(citiesSearchbars, "cities");

        document.querySelectorAll(".passwordVis").forEach((el) => {
          el.addEventListener("click", (e) => showPassword(e));
        });

        const steps = [
          document.querySelector(".form--step--1"),
          document.querySelector(".form--step--2"),
          document.querySelector(".form--step--3"),
        ];
        const stepHeight = getAbsoluteHeight(steps[0]);

        const formBox = document.querySelector(".signup--form");
        let values = [0, 0.5, 1, 1.5];
        if (document.body.offsetWidth > 800)
          values = values.map(function (x) {
            return x * 1.5;
          });
        formBox.addEventListener("scroll", () => {
          if (formBox.scrollTop > values[0] && formBox.scrollTop < stepHeight * values[1]) {
            trackButtons.forEach((b) => b.classList.remove("active"));
            Array.from(trackButtons)[0].classList.add("active");
          } else if (formBox.scrollTop > stepHeight * values[1] && formBox.scrollTop < stepHeight * values[2]) {
            trackButtons.forEach((b) => b.classList.remove("active"));
            Array.from(trackButtons)[1].classList.add("active");
          } else if (formBox.scrollTop > stepHeight * values[2] && formBox.scrollTop < stepHeight * values[3]) {
            trackButtons.forEach((b) => b.classList.remove("active"));
            Array.from(trackButtons)[2].classList.add("active");
          }
        });

        const trackButtons = document.querySelectorAll(".tracker--circle");
        trackButtons.forEach((button) => {
          button.addEventListener("click", (e) => {
            steps[e.target.dataset.number].scrollIntoView({ behavior: "smooth" });
          });
        });

        //adds listener for each input to update completion tracker
        inputs.forEach((el) => {
          el.forEach((input) => {
            if (input.dataset.listener !== "true") {
              inputTracking(inputs[input.dataset.number], input);
            }
          });
        });

        const height = getAbsoluteHeight(".measure-wd") - getAbsoluteHeight("header") - 60;
        document.querySelector(".wrapper--signup").style.height = `${height}px`;
      });
    </script>
  </body>
</html>
